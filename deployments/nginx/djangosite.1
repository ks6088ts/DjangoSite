server {
    listen      80;
    server_name 160.16.242.179; # substitute your machine's IP address or FQDN

    # Django media
    location /media/  {
        root /home/ubuntu/djangosite/www/djangosite;
    }

    location /static/ {
        root /home/ubuntu/djangosite/www/djangosite;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn/djangosite.sock;
    }
}
